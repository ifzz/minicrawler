prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: libminicrawler
Description: Single-threaded multi-socket HTTP(S) client

Version: @VERSION@
Requires.private: libcares >= 1.7.0, libssl >= 1.0.1, libcrypto >= 1.0.1, libnghttp2 >= 1.5.0
URL: https://github.com/testomato/minicrawler
Libs: -L${libdir} -lminicrawler-@MINICRAWLER_API_VERSION@
Cflags: -I${includedir}/minicrawler-@MINICRAWLER_API_VERSION@
